; Ouput directories for files & shortcuts
[DestinationDirs]
Files.Windows = 	0,%CE2%

; Install into \Windows
[Files.Windows]
"NSBWin32.dll",,,0x00000010


; Create document associations...
[AddRegistry.All]

; Misc. strings...
[Strings]
Provider = "NSB"
