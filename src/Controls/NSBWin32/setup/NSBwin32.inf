[DefaultInstall.PPC_SH3]
CESelfRegister=NSBWin32.dll

[DefaultInstall.PocketPC_MIPS]
CESelfRegister=NSBWin32.dll

[DefaultInstall.PocketPC_SH3]
CESelfRegister=NSBWin32.dll

[DefaultInstall.PocketPC_ARM]
CESelfRegister=NSBWin32.dll

[DefaultInstall.HPC2000_MIPS]
CESelfRegister=NSBWin32.dll

[DefaultInstall.HPC2000_ARM]
