Sub Form1_Load
   NSListView1.Icons.Width = 24
    NSListView1.Icons.Height = 24
    NSListView1.Icons.LoadBitmap "iml.bmp"
    NSListView1.SmallIcons.Width = 24
    NSListView1.SmallIcons.Height = 24
    NSListView1.SmallIcons.LoadBitmap "iml.bmp"
    NSListView1.SubItemImages = True
    NSListView1.AllowColumnReorder = True
    NSListView1.UpdateIcons
    NSListView1.Checkboxes = True
End Sub

Sub CommandButton1_Click
   NSListView1.ListItems.Add , ItemText.Text, ItemText.Text, CLng(ItemImage.Text)
End Sub

Sub CommandButton2_Click
   NSListView1.ColumnHeaders.Add CLng(ColIndex.Text), ColCapt.Text, ColCapt.Text
End Sub

Sub CommandButton4_Click
   Dim I, Itm
   For I = 1 To NSListView1.ListItems.Count
        Set Itm = NSListView1.ListItems.Item(I)
        If Itm.Selected Then
            Itm.SubItems.Item(CLng(SubIndex.Text)).Text = SubText.Text
            Itm.SubItems.Item(CLng(SubIndex.Text)).Icon = CLng(SubImage.Text)
        End If
    Next
End Sub
